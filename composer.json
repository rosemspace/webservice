{
    "name": "rosem/webservice",
    "description": "Rosem Web Framework",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "danielstjules/stringy": "~3.1",
        "doctrine/orm": "^2.7",
        "fig/http-message-util": "^1.1",
        "laminas/laminas-diactoros": "^2.3",
        "league/flysystem": "^1.0",
        "league/plates": "dev-master",
        "monolog/monolog": "^2.0",
        "nesbot/carbon": "^2.33",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr7-sessions/storageless": "^7.0",
        "symfony/console": "^5.1",
        "symfony/finder": "^5.0",
        "symfony/polyfill-mbstring": "1.15.*",
        "tedivm/stash": "0.15.*",
        "tijsverkoyen/css-to-inline-styles": "^2.2",
        "vlucas/phpdotenv": "^4.1",
        "webonyx/graphql-php": "^0.13"
    },
    "require-dev": {
        "container-interop/service-provider": "^0.4.0",
        "filp/whoops": "^2.7",
        "fzaninotto/faker": "~1.9",
        "kint-php/kint": "^3.2",
        "mockery/mockery": "1.3.*",
        "nikic/fast-route": "1.3.*",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "~3.5.4",
        "symfony/css-selector": "5.0.*",
        "symfony/dom-crawler": "5.0.*",
        "symfony/routing": "~5.0.7",
        "symplify/easy-coding-standard": "^8.2",
        "symplify/monorepo-builder": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Rosem\\Component\\Admin\\": "packages/admin/src",
            "Rosem\\Component\\App\\": "packages/app/src",
            "Rosem\\Component\\Authentication\\": "packages/authentication/src",
            "Rosem\\Component\\Container\\": [
                "packages/container-service-provider-util/src",
                "packages/container/src"
            ],
            "Rosem\\Component\\Encryption\\": "packages/encryption/src",
            "Rosem\\Component\\Filesystem\\": "packages/filesystem/src",
            "Rosem\\Component\\Hash\\": "packages/hash/src",
            "Rosem\\Component\\Http\\Authentication\\": "packages/http-authentication/src",
            "Rosem\\Component\\Http\\Server\\": [
                "packages/http-server-emitter/src",
                "packages/http-server-handler-util/src",
                "packages/http-server-middleware/src",
                "packages/http-server/src"
            ],
            "Rosem\\Component\\Route\\": "packages/route/src",
            "Rosem\\Component\\Template\\": "packages/template/src",
            "Rosem\\Contract\\App\\": "packages/app-contract/src",
            "Rosem\\Contract\\Authentication\\": "packages/authentication-contract/src",
            "Rosem\\Contract\\Container\\": "packages/container-service-provider-contract/src",
            "Rosem\\Contract\\Debug\\": "packages/debug-contract/src",
            "Rosem\\Contract\\Hash\\": "packages/hash-contract/src",
            "Rosem\\Contract\\Http\\Authentication\\": "packages/http-authentication-contract/src",
            "Rosem\\Contract\\Http\\Server\\": [
                "packages/http-server-emitter-contract/src",
                "packages/http-server-middleware-contract/src"
            ],
            "Rosem\\Contract\\Route\\": "packages/route-contract/src",
            "Rosem\\Contract\\Template\\": "packages/template-contract/src",
            "Rosem\\Utility\\Filesystem\\": "packages/filesystem-util/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rosem\\Component\\Admin\\Tests\\": "packages/admin/tests",
            "Rosem\\Component\\App\\Tests\\": "packages/app/tests",
            "Rosem\\Component\\Authentication\\Tests\\": "packages/authentication/tests",
            "Rosem\\Component\\Container\\Tests\\": [
                "packages/container-service-provider-util/tests",
                "packages/container/tests"
            ],
            "Rosem\\Component\\Encryption\\Tests\\": "packages/encryption/tests",
            "Rosem\\Component\\Filesystem\\Tests\\": "packages/filesystem/tests",
            "Rosem\\Component\\Hash\\Tests\\": "packages/hash/tests",
            "Rosem\\Component\\Http\\Authentication\\Tests\\": "packages/http-authentication/tests",
            "Rosem\\Component\\Http\\Server\\Test\\": "packages/http-server-handler-util/tests",
            "Rosem\\Component\\Http\\Server\\Tests\\": [
                "packages/http-server-emitter/tests",
                "packages/http-server-middleware/tests",
                "packages/http-server/tests"
            ],
            "Rosem\\Component\\Route\\Tests\\": "packages/route/tests",
            "Rosem\\Component\\Template\\Tests\\": "packages/template/tests",
            "Rosem\\Utility\\Filesystem\\Tests\\": "packages/filesystem-util/tests"
        }
    },
    "replace": {
        "rosem/admin": "self.version",
        "rosem/app": "self.version",
        "rosem/app-contract": "self.version",
        "rosem/authentication": "self.version",
        "rosem/authentication-contract": "self.version",
        "rosem/container": "self.version",
        "rosem/container-service-provider-contract": "self.version",
        "rosem/container-service-provider-util": "self.version",
        "rosem/debug-contract": "self.version",
        "rosem/encryption": "self.version",
        "rosem/filesystem": "self.version",
        "rosem/filesystem-util": "self.version",
        "rosem/hash": "self.version",
        "rosem/hash-contract": "self.version",
        "rosem/http": "self.version",
        "rosem/http-authentication": "self.version",
        "rosem/http-authentication-contract": "self.version",
        "rosem/http-server": "self.version",
        "rosem/http-server-emitter": "self.version",
        "rosem/http-server-emitter-contract": "self.version",
        "rosem/http-server-handler-util": "self.version",
        "rosem/http-server-middleware": "self.version",
        "rosem/http-server-middleware-contract": "self.version",
        "rosem/route": "self.version",
        "rosem/route-contract": "self.version",
        "rosem/template": "self.version",
        "rosem/template-contract": "self.version"
    },
    "scripts": {
        "complete-check": [
            "phpunit",
            "@fix-cs",
            "@phpstan"
        ],
        "check-cs": "vendor/bin/ecs check packages --set symplify --ansi",
        "fix-cs": [
            "vendor/bin/ecs check packages --set symplify --fix --ansi",
            "bin/clear_readmes.sh"
        ],
        "phpstan": "vendor/bin/phpstan analyse packages --error-format symplify --ansi",
        "changelog": [
            "packages/changelog-linker/bin/changelog-linker dump-merges --in-categories --in-packages --ansi",
            "packages/changelog-linker/bin/changelog-linker link --ansi",
            "packages/changelog-linker/bin/changelog-linker cleanup --ansi"
        ],
        "merge": "vendor/bin/monorepo-builder merge --ansi",
        "propagate": "vendor/bin/monorepo-builder propagate --ansi",
        "rector": "vendor/bin/rector process --config rector-ci.yaml --dry-run --ansi",
        "validate-monorepo": "vendor/bin/monorepo-builder validate --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
