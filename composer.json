{
    "name": "rosem/webservice",
    "description": "Rosem Web Framework",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "danielstjules/stringy": "~3.1",
        "doctrine/orm": "^2.7",
        "fig/http-message-util": "^1.1",
        "laminas/laminas-diactoros": "^2.2",
        "league/flysystem": "^1.0",
        "league/plates": "dev-master",
        "monolog/monolog": "^2.0",
        "nesbot/carbon": "^2.29",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr7-sessions/storageless": "^5.1",
        "symfony/finder": "^5.0",
        "symfony/polyfill-mbstring": "1.15.*",
        "tedivm/stash": "0.15.*",
        "tijsverkoyen/css-to-inline-styles": "^2.2",
        "vlucas/phpdotenv": "^4.1",
        "webonyx/graphql-php": "^0.13"
    },
    "require-dev": {
        "container-interop/service-provider": "^0.4.0",
        "filp/whoops": "^2.7",
        "fzaninotto/faker": "~1.9",
        "kint-php/kint": "^3.2",
        "mockery/mockery": "0.9.*",
        "nikic/fast-route": "1.3.*",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "~3.5.4",
        "symfony/css-selector": "5.0.*",
        "symfony/dom-crawler": "5.0.*",
        "symfony/routing": "~5.0.7",
        "symplify/monorepo-builder": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Rosem\\Component\\Admin\\": "packages/admin/src",
            "Rosem\\Component\\App\\": "packages/app/src",
            "Rosem\\Component\\Authentication\\": "packages/authentication/src",
            "Rosem\\Component\\Container\\": "packages/container/src",
            "Rosem\\Component\\Hash\\": "packages/hash/src",
            "Rosem\\Component\\Http\\": "packages/http/src",
            "Rosem\\Component\\Route\\": "packages/route/src",
            "Rosem\\Component\\Template\\": "packages/template/src",
            "Rosem\\Contract\\App\\": "packages/app-contract/src",
            "Rosem\\Contract\\Authentication\\": "packages/authentication-contract/src",
            "Rosem\\Contract\\Container\\": "packages/container-service-provider-contract/src",
            "Rosem\\Contract\\Debug\\": "packages/debug-contract/src",
            "Rosem\\Contract\\Hash\\": "packages/hash-contract/src",
            "Rosem\\Contract\\Http\\Server\\": "packages/http-server-emitter-contract/src",
            "Rosem\\Contract\\Route\\": "packages/route-contract/src",
            "Rosem\\Contract\\Template\\": "packages/template-contract/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rosem\\Component\\Admin\\Tests\\": "packages/admin/tests",
            "Rosem\\Component\\App\\Tests\\": "packages/app/tests",
            "Rosem\\Component\\Authentication\\Tests\\": "packages/authentication/tests",
            "Rosem\\Component\\Container\\Tests\\": "packages/container/tests",
            "Rosem\\Component\\Hash\\Tests\\": "packages/hash/tests",
            "Rosem\\Component\\Http\\Tests\\": "packages/http/tests",
            "Rosem\\Component\\Route\\Tests\\": "packages/route/tests",
            "Rosem\\Component\\Template\\Tests\\": "packages/template/tests"
        }
    },
    "replace": {
        "rosem/admin": "self.version",
        "rosem/app": "self.version",
        "rosem/app-contract": "self.version",
        "rosem/authentication": "self.version",
        "rosem/authentication-contract": "self.version",
        "rosem/container": "self.version",
        "rosem/container-service-provider-contract": "self.version",
        "rosem/debug-contract": "self.version",
        "rosem/hash": "self.version",
        "rosem/hash-contract": "self.version",
        "rosem/http": "self.version",
        "rosem/http-server-emitter-contract": "self.version",
        "rosem/route": "self.version",
        "rosem/route-contract": "self.version",
        "rosem/template": "self.version",
        "rosem/template-contract": "self.version"
    }
}
