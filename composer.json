    "fzaninotto/faker": "~1.9",
    "kint-php/kint": "^3.2",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-master",
    "symfony/css-selector": "5.0.*",
    "symfony/dom-crawler": "5.0.*",
    "symfony/routing": "^5.0",
    "squizlabs/php_codesniffer": "~3.5.4"
