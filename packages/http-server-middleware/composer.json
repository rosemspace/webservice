{
    "name": "rosem/http-server-middleware",
    "description": "HTTP server middleware library",
    "keywords": [
        "rosem",
        "http",
        "server",
        "middleware",
        "http-middleware",
        "http-server",
        "http-server-middleware",
        "psr",
        "psr-7",
        "psr-15"
    ],
    "homepage": "https://rosem.space/http-server-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "roshecode",
            "email": "iroman.via@gmail.com",
            "homepage": "https://rosem.space",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Rosem\\Component\\Http\\Server\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rosem\\Component\\Http\\Server\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
