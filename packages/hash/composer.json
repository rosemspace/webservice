{
  "name": "rosem/hash",
  "description": "Hash generation and verification library",
  "homepage": "https://rosem.space/container",
  "license": "MIT",
  "keywords": [
    "hash"
  ],
  "authors": [
    {
      "name": "ma1oy",
      "email": "iroman.via@gmail.com",
      "homepage": "https://rosem.space",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "rosem/hash-contract": "^0.0.1",
    "symfony/polyfill-mbstring": "1.15.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
