{
  "name": "rosem/http",
  "description": "HTTP library",
  "homepage": "https://rosem.space/http",
  "license": "MIT",
  "keywords": ["http"],
  "authors": [
    {
      "name": "roshecode",
      "email": "iroman.via@gmail.com",
      "homepage": "https://rosem.space",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "psr/container": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Rosem\\Component\\Http\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rosem\\Component\\Http\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
