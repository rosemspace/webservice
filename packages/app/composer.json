{
  "name": "rosem/app",
  "type": "library",
  "description": "Application library",
  "keywords": ["app"],
  "homepage": "https://rosem.space/app",
  "license": "MIT",
  "authors": [
    {
      "name": "roshecode",
      "email": "iroman.via@gmail.com",
      "homepage": "https://rosem.space",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "danielstjules/stringy": "~3.1",
    "doctrine/orm": "^2.7",
    "fig/http-message-util": "^1.1",
    "league/flysystem": "^1.0",
    "league/plates": "dev-master",
    "monolog/monolog": "^2.0",
    "nesbot/carbon": "^2.29",
    "psr/container": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr7-sessions/storageless": "^5.1",
    "symfony/finder": "^5.0",
    "tedivm/stash": "0.15.*",
    "tijsverkoyen/css-to-inline-styles": "^2.2",
    "vlucas/phpdotenv": "^4.1",
    "webonyx/graphql-php": "^0.13",
    "laminas/laminas-diactoros": "^2.2"
  },
  "require-dev": {
    "filp/whoops": "^2.7",
    "fzaninotto/faker": "~1.9",
    "kint-php/kint": "^3.2",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-master",
    "symfony/css-selector": "5.0.*",
    "symfony/dom-crawler": "5.0.*",
    "squizlabs/php_codesniffer": "~3.5.4"
  },
  "autoload": {
    "psr-4": {
      "Rosem\\Component\\App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rosem\\Component\\App\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
